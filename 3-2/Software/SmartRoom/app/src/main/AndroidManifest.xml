<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akibgolap.kuet.SmartRoom" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.akibgolap.kuet.SmartRoom.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.akibgolap.kuet.SmartRoom.Main22Activity"
            android:label="@string/title_activity_main22" >
        </activity>
        <activity
            android:name="com.akibgolap.kuet.SmartRoom.Main2Activity"
            android:label="@string/title_activity_main2" >
        </activity>
    </application>

</manifest>
